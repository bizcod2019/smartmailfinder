{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "STREAMLIT_SERVER_PORT": "8501",
    "STREAMLIT_SERVER_ADDRESS": "0.0.0.0"
  },
  "functions": {
    "app.py": {
      "runtime": "@vercel/python@4.1.0",
      "maxDuration": 60
    }
  }
}